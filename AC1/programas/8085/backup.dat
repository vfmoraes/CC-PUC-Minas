MVI C, 00H    ; Initialize counter
LXI H, 0000H  ; Initialize result

; Load first number
LDA 2050H
MOV B, A

; Load second number
LDA 2051H
MOV A, C

BACK: ADD B     ; Add the first number to itself
JC NEXT         ; If carry, increment H
INR H
NEXT: DCR A     ; Decrement the second number
JNZ BACK        ; If not zero, repeat

; Store result
SHLD 2052H
HLT             ; Halt program

